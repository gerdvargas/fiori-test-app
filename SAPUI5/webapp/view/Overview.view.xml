<mvc:View controllerName="DTT.SAPUI5.controller.App" 
xmlns="sap.m" 
xmlns:mvc="sap.ui.core.mvc" 
displayBlock="true">
    <Page title="{i18n>appTitle}">
        <headerContent>
            <Button id="HelloDialogHeader" 
                    icon="sap-icon://action" 
                    press=".onOpenDialogHeader" 
                    class="sapUiSmallMarginEnd" />
        </headerContent>
        <content>
            <mvc:XMLView id="_IDGenXMLView1" viewName="DTT.SAPUI5.view.HelloPanel" />
            <mvc:XMLView id="_IDGenXMLView2" viewName="DTT.SAPUI5.view.InvoicesList" />
        </content>
    </Page>
</mvc:View>
